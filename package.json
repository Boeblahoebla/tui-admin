{
  "name": "tui-admin",
  "version": "1.0.0",
  "description": "A preliminary user administration app to apply for a job @ Tui",
  "main": "server.js",
  "scripts": {
    "client-install": "npm install --prefix client",
    "start": "node server.js",
    "server": "nodemon server.js",
    "client": "npm start --prefix client",
    "dev": "concurrently \"npm run client\" \"npm run server\"",
    "heroku-postbuild": "NPM_CONFIG_PRODUCTION=false npm install --prefix client && npm run build --preflix client"
  },
  "author": "Dzengiz Tafa",
  "license": "ISC",
  "dependencies": {
    "@types/classnames": "^2.2.9",
    "@types/react-router": "^5.1.3",
    "bcryptjs": "^2.4.3",
    "body-parser": "^1.19.0",
    "concurrently": "^5.0.2",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "jsonwebtoken": "^8.5.1",
    "mongoose": "^5.8.1",
    "morgan": "^1.9.1",
    "nodemon": "^2.0.2",
    "passport": "^0.4.1",
    "passport-jwt": "^4.0.0",
    "validator": "^12.1.0"
  },
  "proxy": "http://localhost:5000"
}
